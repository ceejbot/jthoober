{
  "name": "jthoober",
  "description": "run bash scripts when you get a push event from a github webhook",
  "version": "1.2.0",
  "author": "C J Silverio <ceejceej@gmail.com>",
  "contributors": [
    "Joey Baker <joey@byjoeybaker.com>",
    "C J Silverio <ceejceej@gmail.com>"
  ],
  "bin": {
    "jthoober": "server.js"
  },
  "bugs": {
    "url": "https://github.com/ceejbot/jthoober/issues"
  },
  "dependencies": {
    "bistre": "~1.0.1",
    "bole": "~2.0.0",
    "github-webhook-handler": "~0.5.0",
    "mkdirp": "~0.5.0",
    "restify": "~4.0.3",
    "through2": "~2.0.0",
    "touch": "~1.0.0",
    "yargs": "~3.32.0"
  },
  "devDependencies": {
    "eslint": "~1.10.3",
    "mocha": "~2.4.1",
    "must": "~0.13.1",
    "nyc": "~5.6.0",
    "rimraf": "~2.5.0",
    "sinon": "~1.17.2"
  },
  "homepage": "https://github.com/ceejbot/jthoober",
  "keywords": [
    "github",
    "webhook",
    "deploy"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ceejbot/jthoober.git"
  },
  "scripts": {
    "dev": "NODE_ENV=dev node server.js --rules ./example-rules.js --secret sooper-sekrit",
    "lint": "eslint lib/*.js *.js test/*.js",
    "test": "nyc mocha -t 5000 --check-leaks --ui exports -R spec test/test-*.js",
    "travis": "npm run lint && nyc mocha -R spec -t 5000 test/test-*.js"
  }
}
